                </TextEquiv>
            </TextLine>
            <TextLine id="tr_1_tl_22" custom="readingOrder {index:21;}">
                <Coords points="771,4302 2983,4306 3045,4200 771,4218"/>
                <Baseline points="787,4285 811,4285 901,4279 990,4273 1080,4273 1169,4273 1259,4273 1349,4273 1438,4273 1528,4273 1617,4267 1707,4267 1796,4267 1886,4273 1975,4273 2065,4273 2154,4273 2244,4273 2333,4273 2423,4273 2513,4267 2602,4267 2692,4267 2781,4273 2871,4267 2960,4267 3050,4267"/>
                <TextEquiv>
